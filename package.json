{
  "name": "skjemautfylling-formio",
  "description": "Inneholder skjema og oversettelser, som pakkes og deployes sammen med fyllut-applikasjonen.",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "pusher": "^3.0.1"
  },
  "devDependencies": {
    "dotenv": "^10.0.0",
    "husky": "^3.1.0",
    "lint-staged": "^10.4.0",
    "prettier": "^2.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,cjs,mjs}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{md,mdx}": [
      "prettier --write"
    ]
  }
}
